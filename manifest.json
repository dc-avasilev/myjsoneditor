{
   "app": {
      "launch": {
         "local_path": "myindex1.html"
      }
   },
   "description": "JSON Editor is a tool to view, edit, and format JSON. It shows your data in an editable treeview and in a code editor.",
   "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png"
   },

   "manifest_version": 2,
   "name": "My JSON Editor",
   "offline_enabled": true,
   "optional_permissions": [ "http://*/", "https://*/" ],
   "permissions": [ "storage", "unlimitedStorage", "webRequest", "webRequestBlocking", "cookies",  "clipboardWrite" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.0"
}
